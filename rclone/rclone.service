[Unit]
Description=Rclone
After=network-online.target

[Service]
Type=simple
ExecStart=/usr/bin/rclone mount od:/ /root/od \
 --vfs-cache-mode off \
 --umask 0000 \
 --default-permissions \
 --allow-non-empty \
 --allow-other \
 --transfers 3 \
 --buffer-size 32M \
 --low-level-retries 200 \
 --vfs-read-chunk-size 32M \
 --vfs-read-chunk-size-limit 1G

[Install]
WantedBy=default.target
